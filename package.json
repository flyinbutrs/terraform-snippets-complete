    "glob": "^7.1.2",
    "task-nibbler": "^1.0.5",
    "vsce": "^1.18.0"
  },
  "scripts": {
    "postversion": "npm run publish:release",
    "publish:vsce": "vsce publish",
    "publish:release": "conventional-github-releaser -p angular -r 0",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
