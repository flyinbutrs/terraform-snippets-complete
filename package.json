{
  "name": "terraform-snippets-complete",
  "displayName": "Complete Terraform Snippets",
  "description": "Autogenerated snippets for all terraform providers",
  "version": "0.0.1",
  "publisher": "Logicworks",
  "author": {
    "name": "Dan Rosenbloom",
    "email": "me@drosenbloom.com"
  },
  "bugs": {
    "url": "https://github.com/flyinbutrs/terraform-snippets-complete/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flyinbutrs/terraform-snippets-complete.git"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "terraform",
        "path": "./snippets/aws.json"
      }
    ]
