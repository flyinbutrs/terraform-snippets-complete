  "icon": "images/logo-header-black@2x-c6b59644.png",
  "galleryBanner": {
    "color": "#822ff7",
    "theme": "dark"
  },
  "license": "MIT",
  "devDependencies": {
    "conventional-changelog": "^1.1.7",
    "conventional-changelog-cli": "^1.3.5",
